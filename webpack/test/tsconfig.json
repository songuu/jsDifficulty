{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "noEmit": false,
    "lib": ["ES5", "ScriptHost", "dom", "dom.iterable", "esnext"],
    "jsx": "react",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "allowJs": true
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"]
}
