  "private": "false",
  "homepage": "",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "vite --mode development",
    "build:beta": "tsc && vite build --mode beta",
    "build:pro": "tsc && vite build --mode production",
    "build:test": "tsc && vite build --mode production && scripts/test.sh",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "lessc ./src/styles/index.less ./dist/index.css",
    "serve": "vite preview",
    "lint:fix": "eslint src/**/*.{ts,tsx,js} --fix --no-error-on-unmatched-pattern --max-warnings 10",
    "lint:prettier": "prettier src/**/*.{css,less,scss,md} --write --ignore-unknown",
    "lint": "npm run lint:prettier && npm run lint:fix",
    "lint:all": "prettier --write . && eslint --fix .",
    "test:unit": "cross-env CI=true jest",
    "test:nowatch": "cross-env CI=true jest",
    "test:ci": "npm run lint && npm run test:nowatch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@types/node": "^18.14.0",
    "antd": "^5.2.2",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "clipboard": "^2.0.11",
    "countup.js": "^2.4.2",
    "js-cookie": "^3.0.1",
    "less-vars-to-js": "^1.3.0",
    "mockjs": "^1.1.0",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-countup": "^6.4.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.10",
    "react-loadable": "^5.5.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "screenfull": "^6.0.2",
    "vite-plugin-importer": "^0.2.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/classnames": "^2.3.1",
    "@types/clipboard": "^2.0.7",
    "@types/jest": "^29.4.0",
    "@types/js-cookie": "^3.0.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.28",
    "@types/react-countup": "^4.3.1",
    "@types/react-custom-scrollbars": "^4.0.10",
    "@types/react-document-title": "^2.0.5",
    "@types/react-dom": "^18.0.11",
    "@types/react-loadable": "^5.5.6",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-jest": "^29.4.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-html": "^3.2.0"
  }
}
