  "private": "false",
  "homepage": "",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "vite --mode development",
    "build:beta": "tsc && vite build --mode beta",
    "build:pro": "tsc && vite build --mode production",
    "build:test": "tsc && vite build --mode production && scripts/test.sh",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "lessc ./src/styles/index.less ./dist/index.css",
    "serve": "vite preview",
    "lint:fix": "eslint src/**/*.{ts,tsx,js} --fix --no-error-on-unmatched-pattern --max-warnings 10",
    "lint:prettier": "prettier src/**/*.{css,less,scss,md} --write --ignore-unknown",
    "lint": "npm run lint:prettier && npm run lint:fix",
    "lint:all": "prettier --write . && eslint --fix .",
    "test:unit": "cross-env CI=true jest",
    "test:nowatch": "cross-env CI=true jest",
    "test:ci": "npm run lint && npm run test:nowatch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.2.13",
    "@types/node": "^16.10.2",
    "antd": "^4.16.13",
    "axios": "^0.21.4",
    "classnames": "^2.3.1",
    "clipboard": "^2.0.8",
    "countup.js": "^2.0.8",
    "js-cookie": "^3.0.1",
    "less-vars-to-js": "^1.3.0",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-countup": "^6.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.12",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "screenfull": "^5.1.0",
    "vite-plugin-importer": "^0.2.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/classnames": "^2.3.1",
    "@types/clipboard": "^2.0.7",
    "@types/jest": "^27.0.2",
    "@types/js-cookie": "^2.2.7",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.24",
    "@types/react-countup": "^4.3.1",
    "@types/react-custom-scrollbars": "^4.0.8",
    "@types/react-document-title": "^2.0.5",
    "@types/react-dom": "^17.0.9",
    "@types/react-loadable": "^5.5.6",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.3.0",
    "@types/react-transition-group": "^4.4.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-jest": "^27.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "husky": "^7.0.2",
    "jest": "^27.2.2",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-html": "^2.1.0"
  }
}
